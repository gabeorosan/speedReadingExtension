{
    "manifest_version": 2,
    "name": "Speed Reader",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["activeTab", "tabs", "storage"],
    "options_page": "options.html",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/16x16.ico",
        "32": "images/32x32.ico",
        "48": "images/48x48.ico",
        "128": "images/128x128.ico"
      },
      "icons": {
        "16": "images/16x16.ico",
        "32": "images/32x32.ico",
        "48": "images/48x48.ico",
        "128": "images/128x128.ico"
      }
    }
  }