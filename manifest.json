{
    "name": "Speed Reader V2",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["tabs", "activeTab", "storage", "scripting", "webNavigation"],
    "background": {
        "service_worker": "background.js"
      },
    "commands": {
      "run-foo": {
        "suggested_key": {
          "default": "Ctrl+Shift+X",
          "mac": "Command+Shift+X"
        },
        "description": "Run \"foo\" on the current page."
      },
      "_execute_action": {
        "suggested_key": {
          "windows": "Ctrl+Shift+U",
          "mac": "Command+Shift+U",
          "chromeos": "Ctrl+Shift+U",
          "linux": "Ctrl+Shift+U"
        }
      }
    },
    "options_page": "options.html",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/16x16.ico",
            "32": "images/32x32.ico",
            "48": "images/48x48.ico",
            "128": "images/128x128.ico"
          },
          "icons": {
            "16": "images/16x16.ico",
            "32": "images/32x32.ico",
            "48": "images/48x48.ico",
            "128": "images/128x128.ico"
          }
      }
    }